#!/usr/sbin/nft -f

table inet filter {
        set whitelist_ips {
                type ipv4_addr
                elements = { <whitelist_of_ips> }
        }

        chain output {
                type filter hook output priority filter; policy accept;
                ip daddr @whitelist_ips meta skgid <restrict_gid> accept
                meta skgid <restrict_gid> drop
                accept
        }
}